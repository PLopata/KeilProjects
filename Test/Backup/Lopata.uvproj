#include <LPC21xx.H>

/* current excercise:  */

void Delay(iMsNumber)
{
	int iDelayFunc;
	int iTeration;
	for(iTeration=0; iTeration  < iMsNumber*1045; iTeration++) 
	{
		iDelayFunc++;
	}
}

int main()
{
	IO1DIR=0x10000;
	while(1)
	{ 
		IO1SET=0x10000;
		Delay(1000);
		IO1CLR=0x10000;
		Delay(1);
	}
	
}
